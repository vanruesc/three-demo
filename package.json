{
	"name": "three-demo",
	"version": "3.2.0",
	"description": "A demo scaffold for three.js.",
	"homepage": "https://github.com/vanruesc/three-demo",
	"main": "build/three-demo.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [
		"demo",
		"scaffold",
		"three",
		"3d"
	],

	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},

	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/three-demo.git"
	},

	"bugs": {
		"url": "https://github.com/vanruesc/three-demo/issues"
	},

	"files": [
		"src",
		"build"
	],

	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:demo && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:demo": "eslint demo/src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"doc": "esdoc",
		"deploy": "cpx demo/assets/** public/demo",
		"pretest": "npm run lint && npm run build:production",
		"test": "npm run ava",
		"prepack": "npm test && inline-import"
	},

	"ava": {
		"failFast": true,
		"files": ["test/**/*.js"]
	},

	"eslintConfig": {
		"extends": "delta"
	},

	"peerDependencies": {
		"dat.gui": ">= 0.0.0 < 1.0.0",
		"synthetic-event": ">= 0.0.0 < 1.0.0",
		"three": ">= 0.89.0 <= 0.97.0"
	},

	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"ava": "0.x.x",
		"browser-env": "3.x.x",
		"cpx": "1.x.x",
		"cross-env": "5.x.x",
		"dat.gui": "0.x.x",
		"esdoc": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "5.x.x",
		"eslint-config-delta": "0.x.x",
		"math-ds": "1.x.x",
		"rollup": "0.x.x",
		"rollup-plugin-babel": "4.x.x",
		"rollup-plugin-babel-minify": "6.x.x",
		"rollup-plugin-node-resolve": "3.x.x",
		"synthetic-event": "0.x.x",
		"stats.js": "0.x.x",
		"three": "0.97.x"
	}
}
