{
	"name": "three-demo",
	"version": "4.0.7",
	"description": "A demo framework for three.js.",
	"homepage": "https://github.com/vanruesc/three-demo",
	"main": "build/three-demo.js",
	"module": "build/three-demo.esm.js",
	"exports": {
		"import": "./build/three-demo.esm.js",
		"require": "./build/three-demo.js"
	},
	"types": "build/types/index.d.ts",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"demo",
		"framework",
		"three",
		"3d"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/three-demo.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/three-demo/issues"
	},
	"files": [
		"build"
	],
	"scripts": {
		"build": "run-s build:*",
		"build:js": "cross-env NODE_ENV=production ts-node-script esbuild",
		"build:dts": "tsc -p tsconfig.d.json",
		"clean": "del-cli public/docs build",
		"dev": "run-p start watch:*",
		"doc": "typedoc",
		"lint": "eslint src demo/src --fix",
		"prepublishOnly": "run-s clean test doc",
		"test": "run-s lint build",
		"start": "hs -s",
		"watch": "run-p watch:*",
		"watch:eslint": "esw -w --ext .ts --color src demo/src",
		"watch:js": "ts-node-script esbuild -w"
	},
	"eslintConfig": {
		"root": true,
		"extends": "aether/typescript",
		"parserOptions": {
			"project": [
				"tsconfig.json"
			]
		}
	},
	"peerDependencies": {
		"dat.gui": ">= 0.0.0 < 1.0.0",
		"three": ">= 0.89.0 < 0.125.0"
	},
	"devDependencies": {
		"@types/dat.gui": "0.x.x",
		"@types/node": "14.x.x",
		"@types/yargs": "15.x.x",
		"@typescript-eslint/eslint-plugin": "4.x.x",
		"@typescript-eslint/parser": "4.x.x",
		"chokidar": "3.x.x",
		"cross-env": "7.x.x",
		"dat.gui": "0.x.x",
		"del-cli": "3.x.x",
		"esbuild": "0.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"eslint-watch": "7.x.x",
		"http-server": "0.x.x",
		"npm-run-all": "4.x.x",
		"three": "0.x.x",
		"ts-node": "9.x.x",
		"tslib": "2.x.x",
		"typedoc": "0.x.x",
		"typescript": "4.x.x",
		"yargs": "16.x.x"
	}
}
