{
	"name": "three-demo",
	"version": "4.0.5",
	"description": "A demo framework for three.js.",
	"homepage": "https://github.com/vanruesc/three-demo",
	"main": "build/three-demo.js",
	"module": "build/three-demo.esm.js",
  "types": "build/three-demo.d.ts",
	"exports": {
		".": "./build/three-demo.esm.js"
	},
	"type": "module",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"demo",
		"framework",
		"three",
		"3d"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/three-demo.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/three-demo/issues"
	},
	"files": [
		"build"
	],
	"scripts": {
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"dev": "run-p start watch:*",
		"doc": "rimraf public/docs && typedoc",
		"lint": "eslint src demo/src --fix",
		"prepublishOnly": "npm test && npm run doc",
		"test": "rimraf build && npm run lint && npm run build:production",
		"start": "hs -s",
		"watch:eslint": "esw -w --ext .ts src demo/src",
		"watch:js": "rollup -c -w"
	},
	"eslintConfig": {
		"root": true,
		"extends": "aether/typescript",
		"parserOptions": {
			"project": [
				"tsconfig.json"
			]
		}
	},
	"peerDependencies": {
		"dat.gui": ">= 0.0.0 < 1.0.0",
		"three": ">= 0.89.0 < 0.124.0"
	},
	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"@rollup/plugin-babel": "5.x.x",
		"@rollup/plugin-node-resolve": "11.x.x",
		"@types/dat.gui": "0.x.x",
		"@typescript-eslint/eslint-plugin": "4.x.x",
		"@typescript-eslint/parser": "4.x.x",
		"cross-env": "7.x.x",
		"dat.gui": "0.x.x",
		"esbuild": "0.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"eslint-watch": "7.x.x",
		"http-server": "0.x.x",
		"npm-run-all": "4.x.x",
		"rimraf": "3.x.x",
		"rollup": "2.x.x",
		"rollup-plugin-dts": "2.x.x",
		"rollup-plugin-esbuild": "2.x.x",
		"three": "0.x.x",
		"tslib": "2.x.x",
		"typedoc": "0.x.x",
		"typescript": "4.x.x"
	}
}
