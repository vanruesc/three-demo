{
	"name": "three-demo",
	"version": "4.0.1",
	"description": "A demo framework for three.js.",
	"homepage": "https://github.com/vanruesc/three-demo",
	"main": "build/three-demo.js",
	"module": "build/three-demo.esm.js",
  "types": "build/types/index.d.ts",
	"exports": {
		".": "./build/three-demo.esm.js"
	},
	"type": "module",
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"demo",
		"framework",
		"three",
		"3d"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/three-demo.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/three-demo/issues"
	},
	"files": [
		"build"
	],
	"scripts": {
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"watch": "rollup -c -w",
		"dev": "concurrently -k -n server,rollup,eslint \"hs -s\" \"rollup -c -w\" \"esw -w --ext .ts src demo/src\"",
		"doc": "rimraf public/docs && typedoc",
		"lint": "eslint src demo/src",
		"prepack": "npm test && npm run doc",
		"test": "npm run lint && rimraf build && npm run build:production"
	},
	"eslintConfig": {
		"root": true,
		"extends": "aether/typescript",
		"parserOptions": {
			"project": [
				"tsconfig.json"
			]
		}
	},
	"peerDependencies": {
		"dat.gui": ">= 0.0.0 < 1.0.0",
		"three": ">= 0.89.0 < 0.121.0"
	},
	"devDependencies": {
		"@rollup/plugin-buble": "0.x.x",
		"@rollup/plugin-node-resolve": "9.x.x",
		"@rollup/plugin-typescript": "5.x.x",
		"@types/dat.gui": "0.x.x",
		"@typescript-eslint/eslint-plugin": "3.x.x",
		"@typescript-eslint/parser": "3.x.x",
		"concurrently": "5.x.x",
		"cross-env": "7.x.x",
		"dat.gui": "0.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"eslint-watch": "7.x.x",
		"http-server": "0.x.x",
		"rimraf": "3.x.x",
		"rollup": "2.x.x",
		"rollup-plugin-terser": "7.x.x",
		"three": "0.x.x",
		"tslib": "2.x.x",
		"typedoc": "0.x.x",
		"typescript": "4.x.x"
	}
}
